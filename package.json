{
    "name": "mikeylab",
    "version": "0.0.1",
    "private": true,
    "license": "AGPL-3.0-only",
    "scripts": {
        "build-partial": "cd client && webpack && cd ..",
        "install-buf": "tag=$(curl 'https://api.github.com/repos/bufbuild/buf/releases/latest' | jq -r '.tag_name'); url=\"https://github.com/bufbuild/buf/releases/download/${tag}/buf-$(uname -s)-$(uname -m)\"; curl -sSL $url -o 'buf' && chmod +x ./buf",
        "duplicate-shared": "rm client/lib/bytes-util.ts client/lib/ip-convert.ts client/lib/ua-parse.ts; cp src/lib/bytes-util.ts src/lib/ip-convert.ts src/lib/ua-parse.ts client/lib",
        "build": "pnpm duplicate-shared; pnpm install-buf && cd proto && ../buf generate && cd ../client && webpack --mode=production && cd ..",
        "deploy": "pnpm build && wrangler deploy",
        "dev": "./dev.sh",
        "start": "pnpm dev",
        "cf-typegen": "wrangler types",
        "test": "jest"
    },
    "devDependencies": {
        "@jest/types": "^30.2.0",
        "@types/brotli": "^1.3.4",
        "@types/jest": "^30.0.0",
        "@types/leaflet": "^1.9.21",
        "@types/node": "^25.0.1",
        "@types/serviceworker": "^0.0.168",
        "@types/webpack": "^5.28.5",
        "copy-webpack-plugin": "^13.0.1",
        "css-loader": "^7.1.2",
        "file-loader": "^6.2.0",
        "jest": "^30.2.0",
        "nanoid": "^5.1.6",
        "rimraf": "^6.1.2",
        "style-loader": "^4.0.0",
        "terser-webpack-plugin": "^5.3.15",
        "ts-jest": "^29.4.6",
        "ts-loader": "^9.5.4",
        "typescript": "^5.5.2",
        "webpack": "^5.103.0",
        "webpack-cli": "^6.0.1",
        "wrangler": "^4.53.0"
    },
    "dependencies": {
        "@bufbuild/buf": "^1.61.0",
        "@bufbuild/protobuf": "^2.10.1",
        "compress-brotli": "^1.3.13",
        "hono": "^4.10.7",
        "idb-keyval": "^6.2.2",
        "isbot": "^5.1.32",
        "leaflet": "^1.9.4"
    }
}